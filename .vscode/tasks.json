{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build active file",
            "command": "C:/MinGW64/bin/gcc.exe",
            "args": [
                "-g",
                "${workspaceFolder}/main.c",
                "-o",
                "${workspaceFolder}/main.exe",
                "-I",
                "${workspaceFolder}/libcurl/include/",
                "-L",
                "${workspaceFolder}/libcurl/lib",
                "-lcurl"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}